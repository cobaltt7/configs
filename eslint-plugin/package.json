{
	"name": "@redguy12/eslint-plugin",
	"version": "4.1.1",
	"description": "Any ESLint rules that I want to use but can't find anywhere.",
	"license": "MIT",
	"author": "Paul Reid",
	"homepage": "https://github.com/redguy12/configs/tree/main/eslint-plugin#readme",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/RedGuy12/configs.git"
	},
	"bugs": {
		"email": "paul@reid-family.org",
		"url": "https://github.com/redguy12/configs/issues"
	},
	"main": "src/index.js",
	"scripts": {
		"format": "prettier --write \"**\" --ignore-unknown && prettier-package-json --write",
		"types": "npx tsc -p jsconfig.json"
	},
	"peerDependencies": {
		"eslint": "8.30.0"
	},
	"devDependencies": {
		"@redguy12/prettier-config": "2.0.2",
		"@types/eslint": "8.4.10",
		"@types/node": "20.12.11",
		"eslint": "8.30.0",
		"prettier": "2.8.0",
		"prettier-package-json": "2.6.0",
		"typescript": "4.9.4"
	},
	"keywords": [
		"eslint",
		"eslint-config",
		"eslint-plugin"
	],
	"prettier": "@redguy12/prettier-config",
	"prettier-package-json": {
		"tabWidth": 4,
		"useTabs": true,
		"expandUsers": true
